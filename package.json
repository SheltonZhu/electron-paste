{
  "name": "electron-paste",
  "version": "1.2.2",
  "author": "Shelton Zhu <498220739@qq.com> (https://github.com/SheltonZhu)",
  "description": "An Electron & Vue.js clipboard with elementUI support.",
  "homepage": "https://github.com/SheltonZhu/electron-paste",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SheltonZhu/electron-paste.git"
  },
  "bugs": {
    "url": "https://github.com/SheltonZhu/electron-paste/issues"
  },
  "keywords": [
    "vue",
    "electron",
    "element-ui",
    "clipboard"
  ],
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder install-app-deps",
    "build:dir": "cross-env DIR=true node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:generate-icons": "electron-icon-builder --input=./build/icons/icon.png --output=build --flatten",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "prettier": "prettier --single-quote --write src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "rebuild": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@sentry/electron": "2.4.1",
    "ajv": "^8.3.0",
    "auto-launch": "^5.0.5",
    "download-git-repo": "^1.0.1",
    "electron-clipboard-extended": "^1.1.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.1.2",
    "electron-vuex": "^1.0.5",
    "element-ui": "^2.15.2",
    "fs-extra": "^5.0.0",
    "md5": "^2.3.0",
    "mousetrap": "^1.6.1",
    "nedb-promises": "^4.1.0",
    "node-window-manager": "^2.2.4",
    "robotjs": "^0.6.0",
    "rxjs": "^5.5.5",
    "sudo-prompt": "^8.1.0",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.5.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@xunlei/vue-context-menu": "^1.0.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^1.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "dayjs": "^1.10.5",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^11.0.0",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^5.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-vue-libs": "^2.1.0",
    "file-loader": "^3.0.1",
    "happypack": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "6.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "prettier": "2.2.1",
    "quill": "^1.3.7",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-fullscreen": "^2.3.5",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-svg-loader": "^0.8.0",
    "vue-template-compiler": "^2.4.2",
    "vuedraggable": "^2.24.3",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,vue,html}": [
      "prettier --single-quote --write src",
      "git add"
    ]
  }
}
